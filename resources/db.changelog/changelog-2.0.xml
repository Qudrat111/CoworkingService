<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="1" author="author">
        <insert tableName="users" schemaName="coworking_service">
            <column name="username" value="admin"/>
            <column name="password" value="adminpass"/>
            <column name="is_admin" value="true"/>
        </insert>
    </changeSet>

    <changeSet id="2" author="author">
        <insert tableName="workplaces" schemaName="coworking_service">
            <column name="location" value="First Floor"/>
        </insert>
    </changeSet>

    <changeSet id="3" author="author">
        <insert tableName="meeting_rooms" schemaName="coworking_service">
            <column name="name" value="Conference Room A"/>
        </insert>
    </changeSet>
</databaseChangeLog>
